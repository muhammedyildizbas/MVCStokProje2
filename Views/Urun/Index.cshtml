@using MvcProje.Models.Entity
@model List<TBLURUNLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ÜRÜN ID</th>
        <th>ÜRÜN ADI</th>
        <th>MARKASI</th>
        <th>KATEGORİ</th>
        <th>FİYAT</th>
        <th>STOK</th>
        
    </tr>
    <tbody>
        @foreach (var urn in Model)
        {
        <tr>
            <td>@urn.URUNID</td>
            <td>@urn.URUNAD</td>
            <td>@urn.MARKA</td>
            <td>@urn.URUNKATEGORİ</td>
            <td>@urn.FIYAT</td>
            <td>@urn.STOK</td>

        </tr>
        }
    </tbody>
    
</table>

